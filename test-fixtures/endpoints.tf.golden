resource "kubernetes_endpoints" "backend" {
  metadata {
    name      = "backend"
    namespace = "default"
    annotations {
      "alpha.istio.io/kubernetes-serviceaccounts" = "default"
    }
  }
  subset {
    address {
      ip = "10.128.0.17"
    }
    port {
      name     = "http-backend"
      port     = 8080
      protocol = "TCP"
    }
  }
  subset {
    not_ready_address {
      ip = "10.128.0.11"
    }
    port {
      name     = "http-backend"
      port     = 8080
      protocol = "TCP"
    }
  }
}
